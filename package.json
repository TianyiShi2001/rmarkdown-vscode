{
  "name": "rmarkdown",
  "publisher": "TianyiShi",
  "author": {
    "name": "Tianyi Shi"
  },
  "icon": "images/icon.png",
  "displayName": "R Markdown",
  "description": "R Markdown Snippets",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TianyiShi2001/rmarkdown-vscode.git"
  },
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:rmarkdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "rmarkdown",
        "extensions": [
          ".Rmd",
          ".Rmarkdown"
        ],
        "aliases": [
          "R Markdown",
          "RMarkdown"
        ]
      }
    ],
    "snippets": [
      {
        "language": "rmarkdown",
        "path": "./snippets.json"
      }
    ],
    "keybindings": [
      {
        "command": "editor.action.insertSnippet",
        "key": "ctrl+alt+i",
        "mac": "cmd+alt+i",
        "when": "editorTextFocus",
        "args": {
          "name": "insert_code_chunk"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  }
}
